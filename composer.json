{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "craftcms/ckeditor": "^4.2.0",
    "craftcms/cms": "5.3.6",
    "nystudio107/craft-imageoptimize": "^5.0.2",
    "nystudio107/craft-seomatic": "^5.1.2",
    "servd/craft-asset-storage": "^4.0.6",
    "spicyweb/craft-embedded-assets": "^5.1.2",
    "verbb/expanded-singles": "^3.0.0",
    "verbb/hyper": "^2.0.5",
    "verbb/image-resizer": "^4.0.1",
    "vlucas/phpdotenv": "^5.4.0",
    "wrav/oembed": "^3.1.1",
    "xpertbot/craft-wheelform": "^4.0.2"
  },
  "require-dev": {
    "craftcms/generator": "^2.0.0",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}
